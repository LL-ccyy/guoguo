
page {
  min-height: 100%;
  background-color: #f2f2f2;
}

.container {
  background-color: #f2f2f2;
  min-height: 100%;
  overflow-x: hidden;
  position: relative;
  width: 100%;
  align-items: stretch;
  padding-bottom: 200rpx;
}

.index{
  width: 95%;
  margin-left: 2.5%;
  margin-top: 30rpx;
}

.rate{
  display: flex;
  flex-direction: row;
  height: 100rpx;
  background-color:white;
}
.title {
  align-items: center;
  padding-top: 16px;
  width: 30%;
  padding-left: 6%;
  height: 30px;
  display: flex;
  font-size: 17px;
}

.star{
  padding-left: 5%;
  display: flex;
  padding-bottom: 10rpx;
  width: 60%;
  align-items: center;
}

.comments{
  padding-top: 15px;
  padding-left: 5%;
  padding-right: 5%;
  width: 100%;
  background-color: white;
  height: 240px;
}


textarea{
  width: 100%;
  /* border-bottom: 2px solid #000000; */
}

.iamgeRootAll {
  margin-top: 10rpx;
  margin-bottom: 70rpx;
  /* display: flex; */
   display: -webkit-flex;
  flex-direction: row;
  margin-left: 2%;
  width: 100%;
  justify-content: flex-start;
  align-items: center;
  flex-wrap: wrap;
}


.imgItem {
  margin-left: 6px;
  margin-right: 6px;
  position: relative;
  width: 66px;
  height: 66px;
  border: 1px solid #000000;
}

.img {
  width: 100%;
  height: 100%;
}

.DeleteImg {
  position: absolute;
  right: 0px;
  width: 16px;
  height: 16px;
}

.photo {
  width: 50%;
  height: 50%;
  margin: 25%;
}

.imgItem text{
  font-size: 12px;
  display: flex;
  padding-left: 13%;
  color: rgb(211, 211, 211);
}

.lines{
  display: flex;
  flex-direction: row;
  background-color: white;
}

.goods-image {
  width: 140rpx;
  height: 140rpx;
  margin-right: 12rpx;
  margin-left: 12rpx;
  background-color: #fafafa;
  border-radius: 10px;
}

.goods-info-box{
  display: flex;
  flex-direction: column;

}

.goods-title{
  font-size: 25rpx;
  color:black;
  font-weight: bold;
  text-align: left;
  word-break: break-all;
  display: -webkit-box; 
height: 50px;	
overflow: hidden;	
text-overflow: ellipsis;
-webkit-line-clamp: 2;
-webkit-box-orient: vertical; 
}

.goods-sub-title{
  font-size: 20rpx;
  color:gray;
  font-weight: 300;
  text-align: left;
  word-break: break-all;
  display: -webkit-box; 
height: 20px;	
overflow: hidden;	
text-overflow: ellipsis;
-webkit-line-clamp: 2;
-webkit-box-orient: vertical; 
}