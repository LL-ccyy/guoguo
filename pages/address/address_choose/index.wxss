.container{         /*cfs 20220520.5.20 16:57 edited*/
  width: 100%;      /*cfs 20220520.5.20 16:57 edited*/
}                   /*cfs 20220520.5.20 16:57 edited*/

.list-box{
  margin-left: 2.5%;
  width: 95%;
  height: 180rpx;
  background-color:white;
  border-radius: 30rpx;
  /* position: fixed; */
  left: 20rpx;
  margin-top: 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 10rpx 20rpx 10rpx 20rpx;
}
.list-wrap{
  width: 85%;
  height: 180rpx;
}
.list-name{
  margin:10rpx 10rpx 10rpx 20rpx;
  font-size: 32rpx;
}
.address-list{
  margin-left: 20rpx;
  display: flex;
}
.address-list .icon-address{
  width: 90rpx;
  height: 50rpx;
  border: 1rpx solid rgb(236, 198, 198);
  border-radius: 20rpx;
  text-align: center;
  font-size: 30rpx;
  margin-right: 20rpx;
  background-color: rgb(212, 240, 202);
}
.txt-address{
  width: 500rpx;
  font-size: 32rpx;
}

.del-view{
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  width: 50rpx;
  height: 100%;
  margin-left: 30rpx;
}

.del-view .del-img{
  width: 50rpx;
  height: 50rpx;
}

.new-btn-box{
  padding-left: 2.5%;
  padding-right: 2.5%;  /*cfs 20220520.5.20 16:57 edited*/
  width: 100%;          /*cfs 20220520.5.20 16:57 edited*/
  height: 80rpx;
  position: fit;
  bottom: 50rpx;
  margin: 0 auto;
  text-align: center;
  z-index: 100;
  display: flex;
  flex-direction: row;
  justify-content: center;
}

.new-btn{
  width: 400rpx;
}

page{
  background-color: rgb(245, 245, 245);
}
.DeleteImg {
  padding-top: 4px;
  /* right: 0; */
  margin-left: 10rpx;
  left: 20rpx;
  width: 30rpx;
  height: 30rpx;
}

.margin{
  height: 100rpx;
}

/* 新建地址弹出层 */
.add-addr-container{
  height: 750rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding-top: 70rpx;
  padding-bottom: 80rpx;
}

.newlist-wrap {
  width: 95%;
  height: 300rpx;
  border-radius: 20rpx;
  display: block;
}

.positiong-list {
  display: flex;
  width: 90%;
  margin-left: 30rpx;
  height: 100rpx;
  border-bottom: 1px solid rgb(197, 194, 194);
}

.newlist-wrap .positiong-list:last-child {
  border-bottom: none;
}

.newlist-name {
  width: 200rpx;
  justify-content: center;
  margin: auto 0;
  padding: 0rpx 0rpx 0rpx 30rpx;
  font-size: small;
}

.detail-name {
  width: 500rpx;
  justify-content: center;
  margin: auto 0;
}

.tag-list {
  display: flex;
  width: 600rpx;
  align-items: center;
  margin: auto 0;
}

.labels {
  width: 100rpx;
  height: 60rpx;
  border: 1px solid rgb(197, 194, 194);
  text-align: center;
  margin-right: 40rpx;
  font-size: small;
  line-height: 60rpx;
}

.labels-active {
  width: 100rpx;
  height: 60rpx;
  border: 1px solid rgb(197, 194, 194);
  text-align: center;
  margin-right: 40rpx;
  font-size: small;
  line-height: 60rpx;
  background-color: rgb(224, 245, 174);
}

/* .flag-one:hover{
  background-color: rgb(224, 245, 174);
} */

.info-detail {
  width: 95%;
  height: 200rpx;
  background-color: white;
  border-radius: 20rpx;
}

.info-detail .positiong-list:last-child {
  border-bottom: none;
}

.input-tag {
  font-size: small;
}

.save-btn-box{
  width: 100%;
  display: inline;
  justify-content: center;
}

.save-btn {
  padding-left: 10%;
  width: 80%;
  line-height: 30rpx;
  text-align: center;
}
